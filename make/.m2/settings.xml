<?xml version="1.0" encoding="UTF-8"?>

<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>naturalis</id>
			<username>${MAVEN_REPO_USER}</username>
			<password>${MAVEN_REPO_PASS}</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>any</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>naturalis</id>
					<name>Naturalis</name>
					<url>http://mvnrepository.naturalis.io:8081/repository/naturalis/</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>naturalis</id>
					<name>Naturalis</name>
					<url>http://mvnrepository.naturalis.io:8081/repository/naturalis/</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>

	</profiles>

</settings>
